  "code_samples": [
    {
      "code": "import { createUpDownMarkers, createChart, LineSeries } from 'lightweight-charts';const chart = createChart('container');const lineSeries = chart.addSeries(LineSeries);const upDownMarkers = createUpDownMarkers(lineSeries, {    positiveColor: '#22AB94',    negativeColor: '#F7525F',    updateVisibilityDuration: 5000,});// to add some dataupDownMarkers.setData(    [        { time: '2020-02-02', value: 12.34 },        //... more line series data    ]);// ... Update some valuesupDownMarkers.update({ time: '2020-02-02', value: 13.54 }, true);// to remove plugin from the seriesupDownMarkers.detach();",
