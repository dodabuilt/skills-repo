      "code": "function myVisibleTimeRangeChangeHandler(newVisibleTimeRange) {    if (newVisibleTimeRange === null) {        // handle null    }    // handle new logical range}chart.timeScale().subscribeVisibleTimeRangeChange(myVisibleTimeRangeChangeHandler);",
