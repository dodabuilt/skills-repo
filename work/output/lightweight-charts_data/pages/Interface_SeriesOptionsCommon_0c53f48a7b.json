      "code": "const firstSeries = chart.addSeries(LineSeries, {    autoscaleInfoProvider: original => {        const res = original();        if (res !== null) {            res.priceRange.minValue -= 10;            res.priceRange.maxValue += 10;        }        return res;    },});",
